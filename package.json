{
  "name": "express-api-typescript",
  "version": "1.0.0",
  "description": "Express API Made in NodeJS and TypeScript",
  "main": "app.js",
  "scripts": {
    "build": "npx tsc --project tsconfig.build.json",
    "start": "set NODE_ENV=production&node dist/app.js",
    "dev": "set NODE_ENV=development&nodemon --files src/app.ts",
    "test": "set NODE_ENV=test&jest --detectOpenHandles --silent --forceExit --watchAll --testTimeout=5000"
  },
  "keywords": [
    "API",
    "APIREST",
    "JSON API",
    "EXPRESS API"
  ],
  "author": "Pedrvisk",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.22",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
